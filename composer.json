{
  "name": "melisplatform/melis-platform-framework-silex",
  "description": "A Silex framework dependecy to for running silex inside Melis Platform",
  "type": "melisplatform-module",
  "license": "OSL-3.0",
  "homepage": "https://github.com/melisplatform/melis-platform-framework-silex",
  "support": {
    "source": "https://github.com/melisplatform/melis-platform-framework-silex",
    "issues": "https://github.com/melisplatform/melis-platform-framework-silex/issues"
  },
  "keywords": [
    "cms",
    "melis",
    "zf2",
    "module"
  ],
  "require": {
    "doctrine/dbal": "~2.2",
    "twig/twig": "^2.11",
    "silex/silex": "~2.0",
    "silex/web-profiler": "~2.0",
    "symfony/asset": "^3.0",
    "symfony/browser-kit": "^3.0",
    "symfony/class-loader": "^3.0",
    "symfony/config": "^3.0",
    "symfony/console": "^3.0",
    "symfony/css-selector": "^3.0",
    "symfony/debug": "^3.0",
    "symfony/finder": "^3.0",
    "symfony/form": "^3.0",
    "symfony/monolog-bridge": "^3.0",
    "symfony/process": "^3.0",
    "symfony/security": "^3.0",
    "symfony/translation": "^3.0",
    "symfony/twig-bridge": "^3.0",
    "symfony/validator": "^3.0",
    "melisplatform/melis-platform-frameworks": "*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "silex": {
      "dont-discover": []
    }
  },
  "minimum-stability": "dev",
  "autoload": {
        "psr-4": {
            "MelisPlatformFrameworkSilex\\": "src/"
        }
    }

}
