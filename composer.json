{
  "name": "melisplatform/melis-platform-framework-silex",
  "description": "A Silex framework dependecy to for running silex inside Melis Platform",
  "type": "melisplatform-module",
  "license": "OSL-3.0",
  "homepage": "https://github.com/melisplatform/melis-platform-framework-silex",
  "support": {
    "source": "https://github.com/melisplatform/melis-platform-framework-silex",
    "issues": "https://github.com/melisplatform/melis-platform-framework-silex/issues"
  },
  "keywords": [
    "cms",
    "melis",
    "zf2",
    "module"
  ],
  "require": {
    "melisplatform/melis-cms-news": "^3.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "doctrine/dbal": "~2.2",
    "twig/twig": "^2.11",
    "silex/silex": "~2.0",
    "silex/web-profiler": "~2.0",
    "symfony/asset": "~2.8|^4.3",
    "symfony/browser-kit": "~2.8|4.3",
    "symfony/class-loader": "~2.8|4.3",
    "symfony/config": "~2.8|4.3",
    "symfony/console": "~2.8|4.3",
    "symfony/css-selector": "~2.8|4.3",
    "symfony/debug": "~2.8|4.3",
    "symfony/finder": "~2.8|4.3",
    "symfony/form": "~2.8|4.3",
    "symfony/monolog-bridge": "~2.8|4.3",
    "symfony/process": "~2.8|4.3",
    "symfony/security": "~2.8|4.3",
    "symfony/translation": "~2.8|4.3",
    "symfony/twig-bridge": "~2.8|4.3",
    "symfony/validator": "~2.8|4.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "silex": {
      "dont-discover": []
    }
  },
  "autoload": {
        "psr-4": {
            "MelisPlatformFrameworkSilex\\": "src/"
        }
    }

}
