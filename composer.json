{
  "name": "melisplatform/melis-platform-framework-silex",
  "description": "A Silex framework dependecy to for running silex inside Melis Platform",
  "type": "melisplatform-module",
  "license": "OSL-3.0",
  "homepage": "https://github.com/melisplatform/melis-platform-framework-silex",
  "support": {
    "source": "https://github.com/melisplatform/melis-platform-framework-silex",
    "issues": "https://github.com/melisplatform/melis-platform-framework-silex/issues"
  },
  "keywords": [
    "cms",
    "melis",
    "zf2",
    "module"
  ],
  "require": {
    "melisplatform/melis-cms-news": "^3.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "doctrine/dbal": "~2.2",
    "twig/twig": "^2.11",
    "silex/silex": "~2.0",
    "silex/web-profiler": "~2.0",
    "symfony/asset": "^4.3",
    "symfony/browser-kit": "4.3",
    "symfony/class-loader": "4.3",
    "symfony/config": "4.3",
    "symfony/css-selector": "4.3",
    "symfony/form": "4.3",
    "symfony/monolog-bridge": "4.3",
    "symfony/security": "4.3",
    "symfony/translation": "4.3",
    "symfony/twig-bridge": "4.3",
    "symfony/validator": "4.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "silex": {
      "dont-discover": []
    }
  },
  "autoload": {
        "psr-4": {
            "MelisPlatformFrameworkSilex\\": "src/"
        }
    }

}
